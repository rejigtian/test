<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RoundImageView">
    <attr name="border_thickness" format="dimension" />
    <attr name="border_inside_color" format="color" />
    <attr name="border_outside_color" format="color" />
</declare-styleable>

    <declare-styleable name="SimpleTextView">
        <attr name="android:text" format="string" />
        <attr name="android:gravity">
            <enum name="start" value="0"/>
            <enum name="center" value="1"/>
            <enum name="end" value="2"/>
        </attr>
        <attr name="android:textColor" />
        <attr name="android:textSize" />
        <attr name="linePadding" format="dimension" />
        <attr name="android:paddingRight" />
        <attr name="android:paddingLeft" />
        <attr name="android:paddingTop" />
        <attr name="android:paddingBottom" />
    </declare-styleable>
    <declare-styleable name="RoundRectProgressBar">
        <attr name="barStartColor" format="color" />
        <attr name="barEndColor" format="color" />
        <attr name="barStartLocation">
            <enum name="LeftTop" value="0" />
            <enum name="RightTop" value="1" />
            <enum name="RightBottom" value="2" />
            <enum name="LeftBottom" value="3" />
        </attr>
        <attr name="barWidth" format="dimension" />
        <attr name="barHeight" format="dimension" />
        <attr name="corner" format="dimension" />
        <attr name="strokeWidth" format="dimension" />
    </declare-styleable>
</resources>